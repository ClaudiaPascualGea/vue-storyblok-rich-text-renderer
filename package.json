{
  "name": "storyblok-rich-text-vue-renderer",
  "version": "0.1.0",
  "description": "Vue.js plugin to render rich text content from Storyblok",
  "author": {
    "name": "Marvin Rudolph",
    "email": "info@marvin-rudolph.de"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:types",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name storyblok-rich-text-vue-renderer src/plugin/index.ts",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "release": "yarn build && standard-version && git push --follow-tags && npm publish --access public"
  },
  "main": "dist/storyblok-rich-text-vue-renderer.common.js",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "core-js": "^2.6.10",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@types/jest": "^24.0.11",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "commitlint-config-gitmoji": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.1.0",
    "standard-version": "^7.0.1",
    "storyblok-js-client": "1.0.34",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "keywords": [
    "storyblok",
    "vue",
    "rich",
    "text",
    "resolve",
    "plugin"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarvinRudolph/storyblok-rich-text-vue-resolver"
  },
  "typings": "dist/plugin/index.d.ts"
}
